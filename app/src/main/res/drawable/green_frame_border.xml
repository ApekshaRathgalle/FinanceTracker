<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">

    <solid android:color="#FFFFFF" /> <!-- Inside of frame (white) -->

    <stroke
        android:width="8dp"
        android:color="#4CAF50" /> <!-- Border color: Green -->

    <padding
        android:left="32dp"
        android:top="32dp"
        android:right="32dp"
        android:bottom="32dp" />

    <corners android:radius="16dp" />
</shape>
